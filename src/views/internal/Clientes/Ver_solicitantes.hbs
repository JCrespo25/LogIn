<link rel="stylesheet" href="/internal/Clientes/Ver_solicitudes/css/verSolicitudes.css">


    <div class="table">
        <div class="table-responsive-xl">
            <table class="table">
                <thead class="table">
                    <tr>
                        <th scope="col">IDe</th>
                        <th scope="col">IDa</th>
                        <th scope="col">Empresa</th>
                        <th scope="col">Representante</th>
                        <th scope="col">Correo</th>
                        <th class="delete"></th> 
                    </tr>
                </thead>
                {{#each solicitantes}}
                        <tr  id="modal-information" data-target=".bd-example-modal-lg"> 
                            <th scope="row">{{_id}}</th>
                            <td>{{id_autoIncrement}}</td>
                            <td>{{empresaName}}</td>
                            <td>{{representanteName}} </td>
                            <td>{{contactoCorreo}}</td>
                            <td>
                                <button type="button" class="btn btn-danger" data-dismiss="modal" data-placement="left" title="Eliminar"><i class="fas fa-trash"></i></button>
                                <button type="button" class="btn btn-warning" data-dismiss="modal" data-placement="left" title="Detalles"
                                id="id_Modal" onclick="respaldoInformacion()" data-toggle="modal" data-target=".bd-example-modal-lg" 
                                    data-id="{{_id}}"
                                    data-empresaname="{{empresaName}}" 
                                    data-empresatradename="{{empresaTradeName}}"
                                    data-representantename="{{representanteName}}" 
                                    data-representanteappaterno="{{representanteApPaterno}}"
                                    data-representanteapmaterno="{{representanteApMaterno}}" 
                                    data-contactonumberfijo="{{contactoNumberFijo}}"
                                    data-contactocelular="{{contactoCelular}}"
                                    data-contactocorreo="{{contactoCorreo}}"
                                    data-fechasolicitud="{{FechaSolicitud}}"
                                    data-autoincrement="{{id_autoIncrement}}">
                                    <i class="fas fa-share-square"></i>
                                </button>
                            </td>
                        </tr>
                    {{/each}}
            </table>
        </div>
    </div>

    <form action="/SYS/clientes/create/cliente" method="POST">
        <!-- ////////////////////////////////////////////////////MODAL PASO 1/////////////////////////////////////////////////////////////// -->
        <div onkeyup="valuesGet()" onkeypress="valuesGet()" onkeyup="valuesGet()" class="modal fade bd-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
            <div class="modal-dialog modal-lg">
                <div class="modal-content">
                    <div class="modal-content">
                        <!-- /////////////HEADER MODAL/////////// -->
                        <div class="modal-header">
                            <h5 class="modal-title" id="exampleModalLabel">Paso 1: Confirmación</h5>
                            <div class="menu-header">
                                <button id="TryEdit" onclick="disabledFalse()" type="button" class="btn btn-primary" title="Editar"><i class="far fa-edit"></i></button>
                                <button type="button" class="btn btn-danger" data-dismiss="modal" data-toggle="tooltip" data-placement="left" title="Eliminar"><i class="fas fa-trash"></i></button>
                                <button type="button" class="btn-exit" data-dismiss="modal" data-toggle="tooltip" data-placement="left" title="Cerrar"><i class="fas fa-times"></i></button>
                            </div>
                        </div>
                        <!-- ////////////////////////BODY MODAL////////////////////////// -->
                        <div class="modal-body1">
                            <h5 class="tittle-empresa">Empresa</h5>
                            <div class="company-box"><input name="empresaRazonSocial" type="text" class="form-control" id="empresaName" readonly=""></div>
                            <div class="trade-box"><input name="empresaNombreComercial" type="text" class="form-control" id="empresaTradeName" readonly=""></div>
                            <h5 class="tittle-representante">Representante</h5>
                            <div class="name-box"><input name="representanteName" type="text" class="form-control" id="representanteName" readonly=""></div>
                            <div class="Lname-box"><input name="representanteApellidoPaterno" type="text" class="form-control" id="representanteApPaterno" readonly=""></div>
                            <div class="Lscndname-box"><input name="representanteApellidoMaterno" type="text" class="form-control" id="representanteApMaterno" readonly=""></div>
                            <h5 class="title-contacto">Contacto</h5>
                            <div class="phone-box"><input name="contactoNumeroFijo" type="text" class="form-control" id="contactoNumberFijo" readonly=""></div>
                            <div class="cellphone-box"><input name="contactoNumeroCelular" type="text" class="form-control" id="contactoCelular" readonly=""></div>
                            <div class="mail-box"><input name="contactoCorreo" type="text" class="form-control" id="contactoCorreo" readonly=""></div>
                            <h5 class="title-cuenta">Cuenta</h5>
                            <div class="date-request"><input name="fechaSolicitud" type="text" class="form-control" id="dateRequest" placeholder="{{solicitante.FechaSolicitud}}" readonly=""></div>
                            <div class="ide-box"><input name="IDe" type="text" class="form-control" id="IDe" placeholder="{{solicitante._id}}" readonly=""></div>
                            <div class="ida-box"><input name="IDa" type="text" class="form-control" id="IDa" placeholder="{{solicitante.id_autoIncrement}}" readonly=""></div>
                            <div class="user"><input name="userName" type="text" class="form-control" id="user" placeholder="Usuario"></div>
                            <div class="password"><input name="userPassword" type="text" class="form-control" id="password" placeholder="Contraseña"></div>
                            <div class="confirm-password"><input name="userPasswordConfirm" type="text" class="form-control" id="confirmPassword" placeholder="Confirmar Contraseña"></div>
                            <div class="form-group">
                                <select name="userCuestion" id="option-question" class="form-control" id="sel1">
                                    <option>Selecciona una pregunta de seguridad</option>
                                    <option>De que color fue su último vehículo.</option>
                                    <option>La ciudad que te gustaría jubilarte.</option>
                                    <option>El nombre de la primera ciudad que visitó.</option>
                                    <option>Cuál fue el nombre de tu primer mascota.</option>
                                    <option>Cuál es tu comida favorita.</option>
                                    <option>Marca de la primer laptop que compré.</option>
                                </select>
                            </div>
                            <div class="answer"><input name="userAnswer" id="answerOfquestion" type="text" class="form-control" placeholder="Respuesta"></div>
                        </div>
                        <!-- //////////////////////////////FOOTER MODAL////////////////////////////////////////// -->
                        <div class="modal-footer">
                            <div class="btn-mail"><button type="button" class="btn btn-danger" data-dismiss="modal" data-toggle="tooltip" data-placement="left" title="Mail"><i class="fas fa-envelope"></i></button></div>
                            <div class="btn-reload"><button id="validate-all" type="button" class="btn btn-secondary" title="Editar"><i class="fas fa-sync-alt"></i></button></div>
                                <button id="restore" onclick="disabledTrue()" type="button" class="btn btn-success">
                                    Restablecer
                                    <i class="fas fa-window-restore"></i>
                                </button>
                            <button id="next" type="button" class="btn btn-primary" data-dismiss="modal" data-toggle="modal"  data-target=".bd-example-modal-lg2">
                                <i class="fas fa-arrow-circle-right"></i>
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
   
        <!-- ////////////MODAL PASO 2///////////// -->
        <div class="modal fade bd-example-modal-lg2" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
            <div class="modal-dialog modal-lg">
                <div class="modal-content">
                    <div class="modal-content">
                        <!-- /////////////HEADER MODAL/////////// -->
                        <div class="modal-header">
                            <h5 class="modal-title" id="exampleModalLabel">Paso 2: Datos Fiscales</h5>
                            <div class="menu-header">
                                <button type="button" class="btn-exit" data-dismiss="modal" data-toggle="tooltip" data-placement="left" title="Cerrar"><i class="fas fa-times"></i></button>
                            </div>
                        </div>
                        <!-- ////////////////////////BODY MODAL////////////////////////// -->
                        <div class="modal-body2">
                            {{!-- DATOS MEXICANOS --}}
                            <div class="mexican-flag"></div>
                            <label class="datos-mex">Datos:</label>
                            <div class="folio-fac"><input name="datosFiscalesMEXFolioF" id="mex-FolioF" type="text" class="form-control" placeholder="Folio(F)"></div> 
                            <div class="folio-cp"><input name="datosFiscalesMEXFolioCP" id="mex-FolioCP" type="text" class="form-control" placeholder="Folio(CP)"></div>
                            <div class="nom-empresa"><input name="datosFiscalesMEXNombreEmpresa" id="mex-Empresa" type="text" class="form-control" placeholder="Empresa"></div>
                            <div class="colonia"><input name="datosFiscalesMEXColonia" id="mex-Colonia" type="text" class="form-control" placeholder="Colonia"></div>
                            <div class="calle"><input name="datosFiscalesMEXCalle" id="mex-Calle" type="text" class="form-control" placeholder="Calle"></div>
                            <div class="no-exterior"><input name="datosFiscalesMEXNoExterior" id="mex-NoExterior" type="text" class="form-control" placeholder="No. Exterior"></div>
                            <div class="no-interior"><input name="datosFiscalesMEXNoInterior" id="mex-NoInterior" type="text" class="form-control" placeholder="No. Interior"></div>
                            <div class="entre-calles"><input name="datosFiscalesMEXEntreCalles" id="mex-EntreCalles" type="text" class="form-control" placeholder="Entre calles"></div>
                            <div class="dir-fiscal"><input name="datosFiscalesMEXDireccionFiscal" id="mex-DireccionFiscal" type="text" class="form-control" placeholder="Dirección Fiscal"></div>
                            <div class="rs"><input name="datosFiscalesMEXRazonSocial" id="mex-RazonSocial" type="text" class="form-control" placeholder="Razón Social"></div>
                            <div class="estado"><input name="datosFiscalesMEXEstadoMexicano" id="mex-EstadoMexicano" type="text" class="form-control" placeholder="Estado Mexicano"></div>
                            <div class="ciudad"><input name="datosFiscalesMEXCiudadMexicana" id="mex-ciudadMexicana" type="text" class="form-control" placeholder="Ciudad Mexicana"></div>
                            <div class="rfc"><input name="datosFiscalesMEXRFC" id="mex-RFC" type="text" class="form-control" placeholder="RFC"></div>
                            <div class="tel"><input name="datosFiscalesMEXTelefono" id="mex-Telefono" type="text" class="form-control" placeholder="Teléfono"></div>
                            {{!-- DATOS AMERICANOS --}}
                            <div class="usa-flag"></div>
                            <label class="datos-us">Datos:</label>
                            <div class="folio-cpus"><input name="datosFiscalesEUAFolioCP" id="eua-FolioF" type="text" class="form-control" placeholder="Folio(CP)"></div>
                            <div class="nom-empresaus"><input name="datosFiscalesEUANombreEmpresa" id="eua-Empresa"  type="text" class="form-control" placeholder="Empresa"></div>
                            <div class="coloniaus"><input name="datosFiscalesEUAColonia" id="eua-Colonia" type="text" class="form-control" placeholder="Colonia"></div>
                            <div class="calleus"><input name="datosFiscalesEUACalle" id="eua-Calle" type="text" class="form-control" placeholder="Calle"></div>
                            <div class="no-exteriorus"><input name="datosFiscalesEUANoExterior" id="eua-NoExterior" type="text" class="form-control" placeholder="No. Exterior"></div>
                            <div class="no-interiorus"><input name="datosFiscalesEUANoInterior" id="eua-NoInterior" type="text" class="form-control" placeholder="No. Interior"></div>
                            <div class="entre-callesus"><input name="datosFiscalesEUAEntreCalles" id="eua-EntreCalles" type="text" class="form-control" placeholder="Entre calles"></div>
                            <div class="cp-us"><input name="datosFiscalesEUACodigoPostal" id="eua-CP" type="text" class="form-control" placeholder="Codigo Postal"></div>
                            <div class="tax-id"><input name="datosFiscalesEUATAXID" id="eua-TaxID" type="text" class="form-control" placeholder="Tax ID"></div>
                            <div class="tel-us"><input name="datosFiscalesEUATelefono" id="eua-telefono" type="text" class="form-control" placeholder="Teléfono"></div>
                        </div>
                        <!-- //////////////////////////////FOOTER MODAL////////////////////////////////////////// -->
                        <div class="modal-footer">
                            <button id="mail" type="button" class="btn btn-danger" data-dismiss="modal" data-toggle="tooltip" data-placement="left" title="Mail"><i class="fas fa-envelope"></i></button>
                            <button id="backOne" type="button" class="btn btn-primary" data-dismiss="modal" data-toggle="modal" data-target=".bd-example-modal-lg" onclick="block()"><i class="fas fa-arrow-circle-left"></i></button>
                            <button id="nextTwo" type="button" class="btn btn-primary" data-dismiss="modal" data-toggle="modal" data-target=".bd-example-modal-lg3"><i class="fas fa-arrow-circle-right"></i></button>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- ////////////MODAL PASO 3///////////// -->
        <div class="modal fade bd-example-modal-lg3" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
            <div class="modal-dialog modal-lg">
                <div class="modal-content">
                    <div class="modal-content">
                        <!-- /////////////HEADER MODAL/////////// -->
                        <div class="modal-header">
                            <h5 class="modal-title" id="exampleModalLabel">Paso 3: Módulos</h5>
                            <div class="menu-header">
                                <button type="button" class="btn-exit" data-dismiss="modal" data-toggle="tooltip" data-placement="left" title="Cerrar"><i class="fas fa-times"></i></button>
                            </div>
                        </div>
                        <!-- ////////////////////////BODY MODAL////////////////////////// -->
                        <div class="modal-body3">
                            <h5 class="title-demo">Demo:</h5>
                            <div class="privilegios">
                                <div class="form-check">
                                    <h6 class="form-check-label"><br><input name="checkBoxDEMO1" id="module-demo-1" type="checkbox" class="form-check-input" checked="false">Option 1</h6>
                                    <h6 class="form-check-label"><br><input name="checkBoxDEMO2" id="module-demo-2" type="checkbox" class="form-check-input" checked="false">Option 2</h6>
                                    <h6 class="form-check-label"><br><input name="checkBoxDEMO3" id="module-demo-3" type="checkbox" class="form-check-input" checked="false">Option 3</h6>
                                    <h6 class="form-check-label"><br><input name="checkBoxDEMO4" id="module-demo-4" type="checkbox" class="form-check-input" checked="false">Option 4</h6>
                                    <h6 class="form-check-label"><br><input name="checkBoxDEMO5" id="module-demo-5" type="checkbox" class="form-check-input" checked="false">Option 5</h6>
                                </div>
                            </div>
                            <h5 class="title-otros">Otros:</h5>
                            <div class="privilegios2">
                                <h6 class="form-check-label"><br><input name="checkBoxOTHERS1" id="module-others-1" type="checkbox" class="form-check-input" checked="false">Option 1</h6>
                                <h6 class="form-check-label"><br><input name="checkBoxOTHERS2" id="module-others-2" type="checkbox" class="form-check-input" checked="false">Option 2</h6>
                                <h6 class="form-check-label"><br><input name="checkBoxOTHERS3" id="module-others-3" type="checkbox" class="form-check-input" checked="false">Option 3</h6>
                                <h6 class="form-check-label"><br><input name="checkBoxOTHERS4" id="module-others-4" type="checkbox" class="form-check-input" checked="false">Option 4</h6>
                                <h6 class="form-check-label"><br><input name="checkBoxOTHERS5" id="module-others-5" type="checkbox" class="form-check-input" checked="false">Option 5</h6>
                                <h6 class="form-check-label"><br><input name="checkBoxOTHERS6" id="module-others-6" type="checkbox" class="form-check-input" checked="false">Option 6</h6>
                                <h6 class="form-check-label"><br><input name="checkBoxOTHERS7" id="module-others-7" type="checkbox" class="form-check-input" checked="false">Option 7</h6>
                                <h6 class="form-check-label"><br><input name="checkBoxOTHERS8" id="module-others-8" type="checkbox" class="form-check-input" checked="false">Option 8</h6>
                            </div>
                            <div class="privilegios3">
                                <h6 class="form-check-label"><br><input name="checkBoxOTHERS9" id="module-others-9" type="checkbox" class="form-check-input" checked="false">Option 9</h6>
                                <h6 class="form-check-label"><br><input name="checkBoxOTHERS10" id="module-others-10" type="checkbox" class="form-check-input" checked="false">Option 10</h6>
                                <h6 class="form-check-label"><br><input name="checkBoxOTHERS11" id="module-others-11" type="checkbox" class="form-check-input" checked="false">Option 11</h6>
                                <h6 class="form-check-label"><br><input name="checkBoxOTHERS12" id="module-others-12" type="checkbox" class="form-check-input" checked="false">Option 12</h6>
                                <h6 class="form-check-label"><br><input name="checkBoxOTHERS13" id="module-others-13" type="checkbox" class="form-check-input" checked="false">Option 13</h6>
                                <h6 class="form-check-label"><br><input name="checkBoxOTHERS14" id="module-others-14" type="checkbox" class="form-check-input" checked="false">Option 14</h6>
                                <h6 class="form-check-label"><br><input name="checkBoxOTHERS15" id="module-others-15" type="checkbox" class="form-check-input" checked="false">Option 15</h6>
                                <h6 class="form-check-label"><br><input name="checkBoxOTHERS16" id="module-others-16" type="checkbox" class="form-check-input" checked="false">Option 16</h6>
                            </div>
                        </div>
                        <!-- //////////////////////////////FOOTER MODAL////////////////////////////////////////// -->
                        <div class="modal-footer">
                            <button id="mailModalThree" type="button" class="btn btn-danger" data-dismiss="modal" data-toggle="tooltip" data-placement="left" title="Mail"><i class="fas fa-envelope"></i></button>
                            <button id="backThree" type="button" class="btn btn-primary" data-dismiss="modal" data-toggle="modal" data-target=".bd-example-modal-lg2"><i class="fas fa-arrow-circle-left"></i></button>
                            <button id="restablecerThree" onclick="pruebas()" type="button" class="btn btn-success" data-dismiss="modal">Restablecer<i class="fas fa-window-restore"></i></button>
                            <button type="submit" id="finished" class="btn btn-info">Finalizar<i class="far fa-save fa-md"></i></button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </form>

    <script>
    /*VARIABLES GLOBALES*/
    var _empresaname;
    var _empresatradename;
    var _representantename;
    var _representanteappaterno;
    var _representanteapmaterno;
    var _contactonumberfijo;
    var _contactocelular;
    var _contactocorreo;

    var R_empresaname;
    var R_empresatradename;
    var R_representantename;
    var R_representanteappaterno;
    var R_representanteapmaterno;
    var R_contactonumberfijo;
    var R_contactocelular;
    var R_contactocorreo;

    var _fechasolicitud;
    var _IDe;
    var _IDa;
    var _user;
    var _password;
    var _confirmpassword;
    var _cuestion;
    var _cuestionrequest;

    //MEX
    var _mexFolioF;
    var _mexFolioCP;
    var _mexEmpresa;
    var _mexColonia;
    var _mexCalle;
    var _mexNoExterior;
    var _mexNoInterior;
    var _mexEntreCalles;
    var _mexDireccionFiscal;
    var _mexRazonSocial;
    var _mexEstadoMexicano;
    var _mexCiudadMexicana;
    var _mexRFC;
    var _mexTelefono;
    //EUA
    var _euaFolioF;
    var _euaEmpresa;
    var _euaColonia;
    var _euaCalle;
    var _euaNoExterior;
    var _euaNoInterior;
    var _euaEntreCalles;
    var _euaCP;
    var _euaTaxID;
    var _euatelefono;

    var checkBoxModule_Demo_1;
    var checkBoxModule_Demo_2;
    var checkBoxModule_Demo_3;
    var checkBoxModule_Demo_4;
    var checkBoxModule_Demo_5;
    var checkModule_others_1;
    var checkModule_others_2;
    var checkModule_others_3;
    var checkModule_others_4;
    var checkModule_others_5;
    var checkModule_others_6;
    var checkModule_others_7;
    var checkModule_others_8;
    var checkModule_others_9;
    var checkModule_others_10;
    var checkModule_others_11;
    var checkModule_others_12;
    var checkModule_others_13;
    var checkModule_others_14;
    var checkModule_others_15;
    var checkModule_others_16;

    $(document).on("click", "#id_Modal", function () {
        _empresaname = $(this).data('empresaname');
        _empresatradename = $(this).data('empresatradename');
        _representantename = $(this).data('representantename');
        _representanteappaterno = $(this).data('representanteappaterno');
        _representanteapmaterno = $(this).data('representanteapmaterno');
        _contactonumberfijo = $(this).data('contactonumberfijo');
        _contactocelular = $(this).data('contactocelular');
        _contactocorreo = $(this).data('contactocorreo');
        _fechasolicitud = $(this).data('fechasolicitud');
        _IDe = $(this).data('id');
        _IDa = $(this).data('autoincrement');
        
        $('#empresaName').val(_empresaname);
        $('#empresaTradeName').val(_empresatradename);
        $('#representanteName').val(_representantename);
        $('#representanteApPaterno').val(_representanteappaterno);
        $('#representanteApMaterno').val(_representanteapmaterno);
        $('#contactoNumberFijo').val(_contactonumberfijo);
        $('#contactoCelular').val(_contactocelular);
        $('#contactoCorreo').val(_contactocorreo);
        $('#dateRequest').val(_fechasolicitud);
        $('#IDe').val(_IDe);
        $('#IDa').val(_IDa);

        //INPUTS MEX
        document.getElementById("mex-FolioF").readOnly = false;
        document.getElementById("mex-FolioCP").readOnly = false;
        document.getElementById("mex-Empresa").readOnly = false;
        document.getElementById("mex-Colonia").readOnly = false;
        document.getElementById("mex-Calle").readOnly = false;
        document.getElementById("mex-NoExterior").readOnly = false;
        document.getElementById("mex-NoInterior").readOnly = false;
        document.getElementById("mex-EntreCalles").readOnly = false;
        document.getElementById("mex-DireccionFiscal").readOnly = false;
        document.getElementById("mex-RazonSocial").readOnly = false;
        document.getElementById("mex-EstadoMexicano").readOnly = false;
        document.getElementById("mex-ciudadMexicana").readOnly = false;
        document.getElementById("mex-RFC").readOnly = false;
        document.getElementById("mex-Telefono").readOnly = false;
        //INPUTS EUA
        document.getElementById("eua-FolioF").readOnly = false;
        document.getElementById("eua-Empresa").readOnly = false;
        document.getElementById("eua-Colonia").readOnly = false;
        document.getElementById("eua-Calle").readOnly = false;
        document.getElementById("eua-NoExterior").readOnly = false;
        document.getElementById("eua-NoInterior").readOnly = false;
        document.getElementById("eua-EntreCalles").readOnly = false;
        document.getElementById("eua-CP").readOnly = false;
        document.getElementById("eua-TaxID").readOnly = false;
        document.getElementById("eua-telefono").readOnly = false;
        //BUTTONS FOOTER
        document.getElementById("mail").readOnly = false;
        document.getElementById("backOne").readOnly = false;
        document.getElementById("nextTwo").readOnly = false;
    });

    function respaldoInformacion(){

        R_empresaname = _empresaname;
        R_empresatradename = _empresatradename;
        R_representantename = _representantename;
        R_representanteappaterno = _representanteappaterno;
        R_representanteapmaterno = _representanteapmaterno;
        R_contactonumberfijo = _contactonumberfijo;
        R_contactocelular = _contactocelular;
        R_contactocorreo = _contactocorreo;
    }

    $(document).on("click", "#restore", function () {
        
        $('#empresaName').val(R_empresaname);
        $('#empresaTradeName').val(R_empresatradename);
        $('#representanteName').val(R_representantename);
        $('#representanteApPaterno').val(R_representanteappaterno);
        $('#representanteApMaterno').val(R_representanteapmaterno);
        $('#contactoNumberFijo').val(R_contactonumberfijo);
        $('#contactoCelular').val(R_contactocelular);
